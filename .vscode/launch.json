  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ES6 Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@vue/cli-service/bin/vue-cli-service",
      "args": ["test:unit"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/@vue/cli-service/bin/vue-cli-service",
      "args": ["serve"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Production Build",
      "program": "${workspaceFolder}/node_modules/@vue/cli-service/bin/vue-cli-service",
      "args": ["build"]
    }
  ]
}
